{
  "cleanUrls": true,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/build/generic/web/viewer.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*).(js|css|html|png|jpg|jpeg|svg|woff|woff2)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ],
  "static": {
    "directory": "build/generic/web"
  }
}
