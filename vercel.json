{
  "builds": [
    {
      "src": "gulpfile.js",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "scripts": {
    "build": "npx gulp generic"
  },
  "cleanUrls": true,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/build/generic/build/web/viewer.html"
    }
  ],
  "headers": [
    {
      "source": "/build/(.*).(js|css|html|png|jpg|jpeg|svg|woff|woff2)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}
